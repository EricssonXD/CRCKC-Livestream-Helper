{
  // launch.json
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter for web 8080",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "8080"
      ]
    },
    {
      "name": "Flutter for web Codespace 3000",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "web-server",
        "--web-port",
        "3000"
      ]
    },
    {
      "name": "Windows",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "windows",
      ]
    },
    {
      "name": "Android",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "emulator-5554",
      ]
    }
  ]
}